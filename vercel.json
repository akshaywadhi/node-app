{
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/users.*",
      "dest": "server.js"
    },
   
    {
      "src": "/",
      "dest": "build/index.html"
    }
  ]
}