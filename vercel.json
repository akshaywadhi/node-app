{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/users.*",
      "dest": "/index.js"
    },
    {
      "src": "/.*",
      "dest": "/index.js"
    }
  ]
}
